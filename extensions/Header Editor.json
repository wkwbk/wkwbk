{
	"request": [
		{
			"enable": true,
			"name": "cnbing >> bing",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^http(s?)://cn.bing.com/(.*)",
			"exclude": "",
			"group": "Bing",
			"isFunction": false,
			"action": "redirect",
			"to": "https://www.bing.com/$2"
		}
	],
	"sendHeader": [
		{
			"enable": true,
			"name": "避免 bing 重新定向到 cnbing",
			"ruleType": "modifySendHeader",
			"matchType": "regexp",
			"pattern": "^http(s?)://www\\.bing\\.com/(.*)",
			"exclude": "",
			"group": "Bing",
			"isFunction": false,
			"action": {
				"name": "x-forwarded-for",
				"value": "8.8.8.8"
			}
		}
	],
	"receiveHeader": [],
	"receiveBody": []
}